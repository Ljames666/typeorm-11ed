{
    "name": "typeorm",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "typeorm": "typeorm-ts-node-commonjs",
        "typeorm2": "ts-node-dev --respawn --transpile-only ./node_modules/typeorm/cli.js",
        "Ecreate": "npm run typeorm entity:create src/entities/",
        "Mcreate": "npm run typeorm migration:create src/migrations/",
        "Mgenerate": "npm run typeorm migration:generate -- -d src/database/dataSource",
        "Mrun": "npm run typeorm migration:run -- -d src/database/dataSource.ts",
        "Mrevert": "npm run typeorm migration:revert -- -d src/database/dataSource.ts",
        "Mshow": "npm run typeorm migration:show -- -d src/database/dataSource.ts",
        "build": "npx tsc",
        "start": "node dist/server.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/dotenv": "^8.2.0",
        "cors": "^2.8.5",
        "cpf-cnpj-validator": "^1.0.3",
        "dotenv": "^16.0.3",
        "dotenv-safe": "^8.2.0",
        "express": "^4.18.2",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.16",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/dotenv-safe": "^8.1.2",
        "@types/express": "^4.17.17",
        "@types/node": "^20.1.3",
        "@types/pg": "^8.6.6",
        "@types/uuid": "^9.0.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
}
